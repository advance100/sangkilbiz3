{
	"name": "sangkil/sangkilbiz3",
	"description": "Simple ERP",
	"keywords": ["yii2", "ERP"],
	"homepage": "http://www.sangkilsoft.com/",
	"type": "project",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Mujib Masyhudi",
			"email": "mujib.masyhudi@gmail.com",
			"homepage": "http://www.sangkilsoft.com/",
			"role": "Founder and project lead"
		},
		{
			"name": "Misbahul Munir",
			"email": "misbahuldmunir@gmail.com",
			"role": "Core project development"
		}
	],
	"support": {
		"issues": "https://github.com/sangkil/sangkilbiz3/issues?page=1&state=open",
		"source": "https://github.com/sangkil/sangkilbiz3"
	},
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-jui": "*",
		"yiisoft/yii2-mongodb": "*",
		"mdmsoft/yii2-admin": "*",
		"mdmsoft/yii2-autonumber": "*",
		"mdmsoft/yii2-logger": "*",
		"mdmsoft/yii2-format-converter": "*",
		"mdmsoft/yii2-ar-relation": "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"replace": {
		"sangkil/biz3-app": "self.version",
		"sangkil/biz3-master": "self.version",
		"sangkil/biz3-purchase": "self.version",
		"sangkil/biz3-sales": "self.version",
		"sangkil/biz3-inventory": "self.version",
		"sangkil/biz3-accounting": "self.version",
		"sangkil/biz3-theme-adminlte": "self.version"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"app/runtime",
			"app/web/assets"
		]
	}
}
